<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DBF8F2">


    <ImageView
        android:id="@+id/outerCircle"
        android:layout_width="140dp"
        android:layout_height="150dp"
        android:src="@drawable/circle_background"
        android:layout_marginTop="-20dp"
        android:layout_marginStart="-50dp" />

    <ImageView
        android:id="@+id/innerCircle"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="-50dp"
        android:src="@drawable/circle_background"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="150dp"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/backbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_marginStart="12dp"
        android:layout_marginTop="50dp"
        android:src="@drawable/arrow_back"
        android:background="@android:color/transparent"
        app:tint="#228380" />


    <TextView
        android:id="@+id/text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Booked Sessions"
        android:textStyle="bold"
        android:textColor="#228380"
        android:textSize="30sp"
        android:layout_marginTop="40dp"
        android:layout_marginStart="50dp"/>


    <ImageView
        android:id="@+id/box1"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:background="@drawable/rounded_grey"
        android:layout_marginStart="10dp"
        android:layout_marginTop="200dp"
        android:layout_marginEnd="30dp"
        android:gravity="center"/>

    <TextView
        android:id="@+id/name1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="John Cooper"
        android:layout_toEndOf="@+id/box1"
        android:textStyle="bold"
        android:textColor="#228380"
        android:textSize="20sp"
        android:layout_marginTop="240dp"
        android:layout_marginStart="-20dp"/>

    <TextView
        android:id="@+id/jobtext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UX Designer @ Google"
        android:layout_below="@+id/name1"
        android:textStyle="bold"
        android:textColor="#000000"
        android:textSize="15sp"
        android:layout_marginStart="150dp"/>





    <TextView
        android:id="@+id/textdate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="24th Dec 2023"
        android:layout_toEndOf="@+id/name1"
        android:textStyle="bold"
        android:textColor="#FFEB3B"
        android:textSize="15sp"
        android:layout_marginTop="245dp"
        android:layout_marginStart="40dp"/>


    <ImageView
        android:id="@+id/box2"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_below="@+id/box1"
        android:layout_marginStart="10dp"
        android:layout_marginTop="-130dp"
        android:layout_marginEnd="30dp"
        android:background="@drawable/rounded_grey"
        android:gravity="center"
        android:src="@drawable/imag4" />

    <ImageView
        android:id="@+id/box"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_below="@+id/box1"
        android:layout_marginStart="10dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="30dp"
        android:background="@drawable/rounded_grey"
        android:gravity="center"
        android:src="@drawable/imag4" />

    <TextView
        android:id="@+id/name2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Emma Phillips"
        android:layout_toEndOf="@+id/box2"
        android:textStyle="bold"
        android:textColor="#228380"
        android:textSize="20sp"
        android:layout_marginTop="410dp"
        android:layout_marginStart="-20dp"/>

    <TextView
        android:id="@+id/jobtext2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Android Developer"
        android:layout_below="@+id/name2"
        android:textStyle="bold"
        android:textColor="#000000"
        android:textSize="15sp"
        android:layout_marginStart="150dp"/>

    <TextView
        android:id="@+id/textdate2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="24th Dec 2023"
        android:layout_toEndOf="@+id/name2"
        android:textStyle="bold"
        android:textColor="#FFEB3B"
        android:textSize="15sp"
        android:layout_marginTop="415dp"
        android:layout_marginStart="30dp"/>

</RelativeLayout>